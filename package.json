{
  "name": "@digital-swing/ripple",
  "version": "0.8.1",
  "description": "Ripple effect",
  "type": "module",
  "main": "./dist/index.cjs",
  "exports": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "browser": "./dist/bundle.min.js",
  "unpkg": "./dist/bundle.min.js",
  "types": "./types/index.d.ts",
  "files": [
    "/dist",
    "/types"
  ],
  "sideEffects": false,
  "repository": "git://github.com/digital-swing/ripple.git",
  "author": {
    "name": "Lucas Demea",
    "url": "https://www.digital-swing.com",
    "email": "lucas@digital-swing.com"
  },
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./dist && rollup -c rollup.config.ts --configPlugin typescript",
    "dev": "rollup -c rollup.config.ts --configPlugin typescript -w",
    "test": "jest",
    "cov": "jest --coverage",
    "lint": "npm run lint:js && npm run lint:css",
    "docs:html": "typedoc --plugin none --readme none --out docs/types src/index.ts",
    "docs:md": "typedoc --plugin typedoc-plugin-markdown --readme none --out docs/types src/index.ts",
    "docs:dev": "vuepress dev docs",
    "docs:vue": "vuepress build docs",
    "lint:js": "eslint resources/scripts",
    "lint:css": "stylelint \"resources/**/*.{css,scss,vue}\"",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "color-alpha": "^1.1.3"
  },
  "peerDependencies": {
    "gsap": "^3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.22.15",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.22.20",
    "@babel/preset-typescript": "7.23.0",
    "@commitlint/cli": "17.7.2",
    "@commitlint/config-conventional": "17.7.0",
    "@rollup/plugin-commonjs": "25.0.5",
    "@rollup/plugin-node-resolve": "15.2.2",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "11.1.5",
    "@types/jest": "29.5.5",
    "@types/jest-image-snapshot": "6.2.1",
    "@types/jsdom-screenshot": "3.2.3",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "@vuepress/plugin-back-to-top": "2.0.0-beta.67",
    "eslint": "8.51.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.4.2",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-tsdoc": "0.2.17",
    "eslint-plugin-typescript-sort-keys": "3.0.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-image-snapshot": "6.2.0",
    "jsdom-screenshot": "4.0.0",
    "prettier": "3.0.3",
    "rimraf": "5.0.5",
    "rollup": "3.29.4",
    "rollup-plugin-dts": "6.1.0",
    "rollup-plugin-summary": "2.0.0",
    "sass": "1.69.0",
    "stylelint": "15.10.3",
    "stylelint-config-property-sort-order-smacss": "9.1.0",
    "stylelint-config-standard": "34.0.0",
    "ts-auto-mock": "3.7.1",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tslib": "2.6.2",
    "typedoc": "0.25.2",
    "typedoc-plugin-markdown": "3.16.0",
    "typescript": "5.2.2",
    "vuepress": "2.0.0-beta.67"
  }
}