{
  "name": "@digital-swing/ripple",
  "version": "0.8.1",
  "description": "Ripple effect",
  "type": "module",
  "main": "./dist/index.cjs",
  "exports": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "browser": "./dist/bundle.min.js",
  "unpkg": "./dist/bundle.min.js",
  "types": "./types/index.d.ts",
  "files": [
    "/dist",
    "/types"
  ],
  "sideEffects": false,
  "repository": "git://github.com/digital-swing/ripple.git",
  "author": {
    "name": "Lucas Demea",
    "url": "https://www.digital-swing.com",
    "email": "lucas@digital-swing.com"
  },
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./dist && rollup -c rollup.config.ts --configPlugin typescript",
    "dev": "rollup -c rollup.config.ts --configPlugin typescript -w",
    "test": "jest",
    "cov": "jest --coverage",
    "lint": "npm run lint:js && npm run lint:css",
    "docs:html": "typedoc --plugin none --readme none --out docs/types src/index.ts",
    "docs:md": "typedoc --plugin typedoc-plugin-markdown --readme none --out docs/types src/index.ts",
    "docs:dev": "vuepress dev docs",
    "docs:vue": "vuepress build docs",
    "lint:js": "eslint resources/scripts",
    "lint:css": "stylelint \"resources/**/*.{css,scss,vue}\"",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "color-alpha": "^1.1.3"
  },
  "peerDependencies": {
    "gsap": "^3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.26.10",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.26.9",
    "@babel/preset-typescript": "7.26.0",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@rollup/plugin-commonjs": "28.0.3",
    "@rollup/plugin-node-resolve": "16.0.0",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.1.0",
    "@types/jest": "29.5.14",
    "@types/jest-image-snapshot": "6.4.0",
    "@types/jsdom-screenshot": "4.0.0",
    "@typescript-eslint/eslint-plugin": "8.26.0",
    "@typescript-eslint/parser": "8.26.0",
    "@vuepress/plugin-back-to-top": "2.0.0-rc.82",
    "eslint": "9.22.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-tsdoc": "0.4.0",
    "eslint-plugin-typescript-sort-keys": "3.3.0",
    "husky": "9.1.7",
    "is-ci": "4.1.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-image-snapshot": "6.4.0",
    "jsdom-screenshot": "4.0.0",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "rollup": "4.35.0",
    "rollup-plugin-dts": "6.1.1",
    "rollup-plugin-summary": "3.0.0",
    "sass": "1.85.1",
    "stylelint": "16.15.0",
    "stylelint-config-property-sort-order-smacss": "10.0.0",
    "stylelint-config-standard": "37.0.0",
    "ts-auto-mock": "3.7.4",
    "ts-jest": "29.2.6",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "typedoc": "0.27.9",
    "typedoc-plugin-markdown": "4.4.2",
    "typescript": "5.8.2",
    "vuepress": "2.0.0-rc.20"
  }
}