{
  "name": "@digital-swing/ripple",
  "version": "0.8.0",
  "description": "Ripple effect",
  "type": "module",
  "main": "./dist/index.cjs",
  "exports": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "browser": "./dist/bundle.min.js",
  "unpkg": "./dist/bundle.min.js",
  "types": "./types/index.d.ts",
  "files": [
    "/dist",
    "/types"
  ],
  "sideEffects": false,
  "repository": "git://github.com/digital-swing/ripple.git",
  "author": {
    "name": "Lucas Demea",
    "url": "https://www.digital-swing.com",
    "email": "lucas@digital-swing.com"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "rimraf ./dist && rollup -c rollup.config.ts --configPlugin typescript",
    "dev": "rollup -c rollup.config.ts --configPlugin typescript -w",
    "test": "jest",
    "cov": "jest --coverage",
    "lint": "npm run lint:js && npm run lint:css",
    "docs:html": "typedoc --plugin none --readme none --out docs/types src/index.ts",
    "docs:md": "typedoc --plugin typedoc-plugin-markdown --readme none --out docs/types src/index.ts",
    "docs:dev": "vuepress dev docs",
    "docs:vue": "vuepress build docs",
    "lint:js": "eslint resources/scripts",
    "lint:css": "stylelint \"resources/**/*.{css,scss,vue}\"",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "color-alpha": "^1.1.3"
  },
  "peerDependencies": {
    "gsap": "^3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.19.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@rollup/plugin-commonjs": "24.0.1",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-terser": "0.4.0",
    "@rollup/plugin-typescript": "11.0.0",
    "@types/jest": "29.4.0",
    "@types/jest-image-snapshot": "6.1.0",
    "@types/jsdom-screenshot": "3.2.2",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "@vuepress/plugin-back-to-top": "2.0.0-beta.60",
    "eslint": "8.33.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-tsdoc": "0.2.17",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jest": "29.4.1",
    "jest-environment-jsdom": "29.4.1",
    "jest-image-snapshot": "6.1.0",
    "jsdom-screenshot": "4.0.0",
    "prettier": "2.8.3",
    "rimraf": "^4.1.2",
    "rollup": "3.12.0",
    "rollup-plugin-dts": "5.1.1",
    "rollup-plugin-summary": "2.0.0",
    "sass": "1.57.1",
    "stylelint": "14.16.1",
    "stylelint-config-property-sort-order-smacss": "9.0.0",
    "stylelint-config-standard": "29.0.0",
    "ts-auto-mock": "3.6.4",
    "ts-jest": "29.0.5",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "typedoc": "0.23.24",
    "typedoc-plugin-markdown": "3.14.0",
    "typescript": "4.9.4",
    "vuepress": "2.0.0-beta.60"
  }
}